    "yaml": "^2.3.1",
    "zod": "^3.21.4"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "unit-tests": "jest ./__tests__/unit",
    "start": "node ./dist/src/index.js",
    "dev": "nodemon ./src/index.ts",
    "prisma:prepare": "npx prisma migrate dev && npx prisma generate client",
